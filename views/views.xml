<odoo>
    <data>

        <record id="action_fiscal_regime" model="ir.actions.act_window">
            <field name="name">Régime fiscal</field>
            <field name="res_model">fiscal.regime</field>
            <field name="view_mode">list,form</field>
        </record>

        <record id="action_fiscal_taxe" model="ir.actions.act_window">
            <field name="name">Impôt</field>
            <field name="res_model">fiscal.taxe</field>
            <field name="view_mode">list,form</field>
        </record>

        <record id="action_adherant" model="ir.actions.act_window">
            <field name="name">adherant</field>
            <field name="res_model">res.partner</field>
            <field name="view_mode">list,form</field>
        </record>

        <record id="action_fiscal_echeance_calendar" model="ir.actions.act_window">
            <field name="name">Calendrier des Échéances Fiscale</field>
            <field name="res_model">fiscal.taxe</field>
            <field name="view_mode">calendar,list,form</field>
        </record>

        <menuitem id="menu_cga_root" name="CGA" sequence="10"/>
        <menuitem id="menu_config" name="Paramètres" sequence="20" parent="menu_cga_root"/>
        <menuitem id="menu_fiscal_regime" name="Régimes Fiscaux" parent="menu_config" action="action_fiscal_regime"
                  sequence="10"/>
        <menuitem id="menu_fiscal_tqxe" name="Obligations" parent="menu_config" action="action_fiscal_taxe"
                  sequence="10"/>
        <menuitem id="menu_adhesion" name="Adhesion CGA" parent="menu_cga_root" sequence="10"/>
        <menuitem id="menu_adherent" name="Dossier Adherent" parent="menu_adhesion" action="action_adherant"
                  sequence="10"/>
        <menuitem id="menu_echeance" name="Calendrier des echeances" parent="menu_adhesion" action="action_fiscal_echeance_calendar"
                  sequence="10" />
